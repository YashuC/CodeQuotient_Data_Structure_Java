class Result {
  int rotationCount(int a[], int size)
  {
    int min_val = a[0], min_index = 0;
        for (int i = 0; i < size; i++)
        {
            if (min_val > a[i])
            {
                min_val = a[i];
                min_index = i;
            }
        } 
        return min_index;
  }
}
